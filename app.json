{
  "expo": {
    "name": "MedGuide",
    "slug": "medguide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.alesasandr.medguide",
      "buildNumber": "1",
      "supportsTablet": true,
      "requirements": [
        "armv7",
        "arm64"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "Требуется доступ к камере для сканирования кодов лекарств",
        "NSPhotoLibraryUsageDescription": "Требуется доступ к фотографиям для загрузки изображений"
      },
      "permissions": [
        "CAMERA",
        "PHOTO_LIBRARY"
      ]
    },
    "android": {
      "package": "com.alesasandr.medguide",
      "versionCode": 1,
      "buildNumber": "1",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-barcode-scanner"
    ],
    "extra": {
      "eas": {
        "projectId": "404ed673-4da2-490e-8dc1-f1f40577a554"
      }
    },
    "owner": "jernix"
  }
}